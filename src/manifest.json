{
  "name": "Djadmin Pro",
  "manifest_version": 3,
  "version": "1.1.0",
  "description": "Help you to like a Pro",
  "content_scripts": [
    {
      "js": [
        "similarity.js",
        "nice-select2.js",
        "content.js",
        "searchTable.js",
        "injects/inject-app-list.js",
        "djquery.js"
      ],
      "css": [
        "nice-select2.css",
        "content.css",
        "djquery.css"
      ],
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*://*.onpoint.vn/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "icons": {
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "96": "img/icon96.png"
  }
}